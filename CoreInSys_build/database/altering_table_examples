
alter table demultiplex add column JID int unsigned;

delete from demultiplex where seqProjectID=4;


# To add a value to an enum, you need to add the entire list.

alter table mytable modify column mycolumn enum ('a','b','c','d','e');

alter table seqProject modify column exptType enum('exome','RNAseq','ChIPseq','other','WGS');


